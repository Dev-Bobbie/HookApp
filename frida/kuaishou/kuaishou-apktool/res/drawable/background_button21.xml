<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape>
            <stroke android:width="@dimen/button_stroke_width" android:color="@color/line_color2_pressed" />
            <solid android:color="@android:color/transparent" />
            <corners android:radius="@dimen/button_radius1" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <stroke android:width="@dimen/button_stroke_width" android:color="@color/line_color2_pressed" />
            <solid android:color="@android:color/transparent" />
            <corners android:radius="@dimen/button_radius1" />
        </shape>
    </item>
    <item android:drawable="@drawable/background_button21_normal" />
</selector>