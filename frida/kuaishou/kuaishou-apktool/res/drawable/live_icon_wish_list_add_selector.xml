<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <bitmap android:src="@drawable/live_icon_wish_list_add" android:dither="true" android:alpha="0.5" />
    </item>
    <item android:state_enabled="false">
        <bitmap android:src="@drawable/live_icon_wish_list_add" android:dither="true" android:alpha="1.0" />
    </item>
    <item>
        <bitmap android:src="@drawable/live_icon_wish_list_add" android:dither="true" android:alpha="1.0" />
    </item>
</selector>