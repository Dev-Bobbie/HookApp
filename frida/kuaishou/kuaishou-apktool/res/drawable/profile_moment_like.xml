<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true" android:state_pressed="true">
        <bitmap android:src="@drawable/profile_icon_like_m_pressed" android:alpha="@integer/q" />
    </item>
    <item android:state_selected="true" android:drawable="@drawable/profile_icon_like_m_pressed" />
    <item android:state_pressed="true">
        <bitmap android:src="@drawable/profile_icon_like_m_normal" android:alpha="@integer/q" />
    </item>
    <item android:state_enabled="false">
        <bitmap android:src="@drawable/profile_icon_like_m_normal" android:alpha="@integer/q" />
    </item>
    <item android:drawable="@drawable/profile_icon_like_m_normal" />
</selector>