<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <bitmap android:src="@drawable/shoot_icon_music_xxl_normal" android:dither="true" android:alpha="0.4" />
    </item>
    <item android:state_selected="true" android:state_pressed="true">
        <bitmap android:src="@drawable/shoot_icon_music_xxl_selected" android:dither="true" android:alpha="0.4" />
    </item>
    <item android:state_selected="true" android:drawable="@drawable/shoot_icon_music_xxl_selected" />
    <item android:state_pressed="true">
        <bitmap android:src="@drawable/shoot_icon_music_xxl_normal" android:dither="true" android:alpha="0.4" />
    </item>
    <item android:drawable="@drawable/shoot_icon_music_xxl_normal" />
</selector>