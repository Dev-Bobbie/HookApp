<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true" android:state_pressed="true">
        <shape>
            <stroke android:width="@dimen/default_divider_size" android:color="@color/p_color_orange_pressed" />
            <solid android:color="@android:color/transparent" />
            <corners android:radius="@dimen/button_radius1" />
        </shape>
    </item>
    <item android:state_selected="true" android:state_pressed="false">
        <shape>
            <stroke android:width="@dimen/default_divider_size" android:color="@color/p_color_orange" />
            <solid android:color="@android:color/transparent" />
            <corners android:radius="@dimen/button_radius1" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <stroke android:width="@dimen/default_divider_size" android:color="@color/line_color2_pressed" />
            <solid android:color="@android:color/transparent" />
            <corners android:radius="@dimen/button_radius1" />
        </shape>
    </item>
    <item>
        <shape>
            <stroke android:width="@dimen/default_divider_size" android:color="@color/line_color2_normal" />
            <solid android:color="@android:color/transparent" />
            <corners android:radius="@dimen/button_radius1" />
        </shape>
    </item>
</selector>